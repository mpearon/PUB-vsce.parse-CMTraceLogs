{
	"name": "vsce-parse-cmtracelogs",
	"displayName": "Parse CMTrace Logs",
	"description": "A simple vsCode extension to parse and display CMTrace-formatted logs",
	"preview": true,
	"version": "0.0.3",
	"publisher": "MPearon",
	"keywords": [
		"mpearon",
		"trigger",
		"words",
		"format",
		"log"
	],
	"author": {
		"name": "Matthew Pearon",
		"url": "https://CL3VER.life/mpearon"
	},
	"homepage": "https://github.com/mpearon/PUB-vsce.parse-CMTraceLogs/blob/master/README.md",
	"icon": "resources/altIcon-256.png",
	"galleryBanner": {
		"color": "#520000",
		"theme": "dark"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/mpearon/PUB-vsce.parse-CMTraceLogs"
	},
	"license": "MIT",
	"extensionKind": [
		"ui"
	],
	"categories": [
		"Formatters"
	],
	"files.associations": {
		"*.log": "log"
	},
	"languages": [
		{
			"id": "log",
			"extensions": [
				".log"
			]
		}
	],
	"activationEvents": [
		"onCommand:extension.parseCMTraceLogs",
		"onLanguage:log"
	],
	"engines": {
		"vscode": "^1.43.0"
	},
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.parseCMTraceLogs",
				"title": "Parse CMTrace Logs"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.12.34",
		"@types/vscode": "^1.43.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.3",
		"tslint": "^5.20.0",
		"typescript": "^3.8.3",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {
		"minimist": "^1.2.5"
	}
}
